# SSH Configuration
# This file is managed in ~/dotfiles and symlinked to ~/.ssh/config

# 1Password SSH Agent
Host *
    IdentityAgent "~/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock"

# Home servers
Host homeserver
    HostName 192.168.3.3
    User clayton

Host fedora
    HostName 192.168.3.4
    User clayton

# ML servers
Host ml
